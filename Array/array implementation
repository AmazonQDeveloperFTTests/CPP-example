#include<iostream>
using namespace std;
void main()
{
    int n;
    cin>>n;
    int* a = new int[n];// declaring array dynamically in heap
    cout<<"Enter values in array :- ";
    for(int i = 0 ; i < n ; i++)
    {
        cin>>a[i];
    }
    cout<<"\n Printing Array :- ";
    for(int i =0 ; i < n ; i++)
    {
        cout<<a[i];
    }
    return 0;
}
